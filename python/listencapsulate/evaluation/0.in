>>> omvat(['a', 'b', 'c'], '*', '*')
['*', 'a', 'b', 'c', '*']

>>> omvat(['zon', 'maan'], '#', '$')
['#', 'zon', 'maan', '$']

>>> omvat(['olifant', 'nijlpaard'], 'begin', 'einde')
['begin', 'olifant', 'nijlpaard', 'einde']

>>> omvat([31, 42], 0, 100)
